{
    "name": "obsidian-life-tracker-base-view-plugin",
    "version": "0.1.0",
    "description": "Adds a Life Tracker View in Bases",
    "author": {
        "name": "SÃ©bastien Dubois",
        "email": "sebastien@developassion.be",
        "url": "https://dsebastien.net"
    },
    "keywords": [
        "obsidian",
        "obsidian-plugin",
        "life-tracking",
        "obsidian-bases",
        "obsidian-base-view"
    ],
    "license": "MIT",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dsebastien/obsidian-life-tracker-base-view.git"
    },
    "bugs": {
        "url": "https://github.com/dsebastien/obsidian-life-tracker-base-view/issues"
    },
    "homepage": "https://github.com/dsebastien/obsidian-life-tracker-base-view",
    "engines": {
        "node": ">=20.0.0",
        "npm": ">=10.1.0"
    },
    "main": "main.js",
    "packageManager": "bun@1.3.2",
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-customizable"
        },
        "cz-customizable": {
            "config": ".cz-config.cjs"
        }
    },
    "scripts": {
        "dev": "bun run build.ts",
        "build": "tsc && bun run build.ts --prod",
        "version": "bun run version-bump.ts && git add manifest.json versions.json",
        "tsc": "tsc --noEmit",
        "tsc:watch": "tsc --noEmit --watch --preserveWatchOutput",
        "tscw": "bun run tsc:watch",
        "lint": "eslint . --max-warnings 0",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "prepare": "husky",
        "commit": "cz",
        "cm": "cz",
        "commit:lint": "commitlint --edit",
        "release": "git tag -a"
    },
    "devDependencies": {
        "@commitlint/cli": "^20.1.0",
        "@commitlint/config-conventional": "^20.1.0",
        "@eslint/js": "^9.36.0",
        "@types/bun": "latest",
        "commitizen": "^4.3.1",
        "concurrently": "9.2.1",
        "cz-customizable": "^7.5.1",
        "eslint": "^9.36.0",
        "eslint-config-prettier": "^10.1.5",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.7",
        "obsidian": "latest",
        "prettier": "^3.6.2",
        "tailwindcss": "4.1.14",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.48.0"
    },
    "dependencies": {
        "immer": "10.0.4",
        "zod": "^4.1.13"
    }
}
